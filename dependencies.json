{
  "jquery": {
    "src": [
      "./node_modules/jquery/dist/jquery.min.js",
      "./node_modules/jquery/dist/jquery.min.map"
    ],
    "dest": "assets/vendor/jquery/",
    "js_link": "assets/vendor/jquery/jquery.min.js"
  },
  
  "bootstrap": {
    "src": "./node_modules/bootstrap/dist/**/*",
    "dest": "assets/vendor/bootstrap/",
    "css_link": "assets/vendor/bootstrap/css/bootstrap.min.css",
    "js_link": "assets/vendor/bootstrap/js/bootstrap.bundle.min.js"
  },
  
  "jquery.easing": {
    "src": "./node_modules/jquery.easing/jquery.easing.min.js",
    "dest": "assets/vendor/jquery.easing/",
    "js_link": "assets/vendor/jquery.easing/jquery.easing.min.js"
  },

  "php-email-form": {
    "js_link": "assets/vendor/php-email-form/validate.js"
  },

  "icofont": {
    "css_link": "assets/vendor/icofont/icofont.min.css"
  },

  "jquery-sticky": {
    "src": "./node_modules/jquery-sticky/jquery.sticky.js",
    "dest": "assets/vendor/jquery-sticky/",
    "js_link": "assets/vendor/jquery-sticky/jquery.sticky.js"
  },

  "boxicons": {
    "srcs": [
      {
        "src": "./node_modules/boxicons/css/*",
        "dest": "assets/vendor/boxicons/css/"
      },
      {
        "src": "./node_modules/boxicons/fonts/*",
        "dest": "assets/vendor/boxicons/fonts/"
      }
    ],
    "css_link": "assets/vendor/boxicons/css/boxicons.min.css"
  },

  "animate.css": {
    "src": "./node_modules/animate.css/*.css",
    "dest": "assets/vendor/animate.css/",
    "css_link": "assets/vendor/animate.css/animate.min.css"
  },

  "venobox": {
    "src": "./node_modules/venobox/venobox/*",
    "dest": "assets/vendor/venobox/",
    "css_link": "assets/vendor/venobox/venobox.css",
    "js_link": "assets/vendor/venobox/venobox.min.js"
  },

  "waypoints": {
    "src": "./node_modules/waypoints/lib/jquery.waypoints.min.js",
    "dest": "assets/vendor/waypoints/",
    "js_link": "assets/vendor/waypoints/jquery.waypoints.min.js"
  },

  "counterup": {
    "js_link": "assets/vendor/counterup/counterup.min.js"
  },

  "isotope-layout": {
    "src": "./node_modules/isotope-layout/dist/**/*",
    "dest": "assets/vendor/isotope-layout/",
    "js_link": "assets/vendor/isotope-layout/isotope.pkgd.min.js"
  },

  "aos": {
    "src": "./node_modules/aos/dist/**/*",
    "dest": "assets/vendor/aos/",
    "css_link": "assets/vendor/aos/aos.css",
    "js_link": "assets/vendor/aos/aos.js"
  }
}
